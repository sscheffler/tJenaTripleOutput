<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.1" STATUS="DEVELOPMENT"

		COMPATIBILITY="ALL" AUTHOR="Stefan Scheffler" RELEASE_DATE="20070525A"
		STARTABLE="false">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>semanticWeb/Jena</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_OUTPUT="0" MIN_INPUT="1"
			MAX_INPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>

		<PARAMETER NAME="USE_FUSEKI" FIELD="RADIO" REQUIRED="true"
			NUM_ROW="1" GROUP="db_type">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_VIRTUOSO" FIELD="RADIO" REQUIRED="true"
			NUM_ROW="1" GROUP="db_type">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		

		<PARAMETER NAME="FUSEKI_CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tFusekiConnection" NUM_ROW="1"
			DYNAMIC_SETTINGS="true" GROUP="db_type" SHOW_IF="USE_FUSEKI == 'true'" />

		<PARAMETER NAME="VIRTUOSO_CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tVirtuosoConnection" NUM_ROW="1"
			DYNAMIC_SETTINGS="true" GROUP="db_type" SHOW_IF="USE_VIRTUOSO == 'true'" />
		<PARAMETER NAME="USE_GLOBALNAMESPACEMAP_CHECK" FIELD="CHECK"
			REQUIRED="true" NUM_ROW="1">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5"
			READONLY="true">
			<TABLE>
				<COLUMN NAME="subject" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="predicate" TYPE="id_String" LENGTH="255" />
				<COLUMN NAME="object" TYPE="id_String" LENGTH="255" />
			</TABLE>
		</PARAMETER>
		
		<PARAMETER NAME="USE_GLOBALNAMESPACEMAP_CHECK" FIELD="CHECK" REQUIRED="true" NUM_ROW="5" >
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="DATA_ACTION" FIELD="CLOSED_LIST" NUM_ROW="10">
			<ITEMS DEFAULT="INSERT">
				<ITEM NAME="INSERT" VALUE="INSERT" />
				<ITEM NAME="DELETE" VALUE="DELETE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="OFFSET_CHECK" FIELD="CHECK" REQUIRED="true"
			NUM_ROW="15">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="OFFSET_FIELD" FIELD="TEXT" NUM_ROW="15"
			REQUIRED="true" SHOW_IF="OFFSET_CHECK == 'true'">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS />

	<CODEGENERATION>
		<IMPORTS>
			<IMPORT NAME="fuseki" MODULE="fuseki-server.jar" REQUIRED="true" />
			<IMPORT NAME="virt_jena" MODULE="virt_jena_own.jar" REQUIRED="true" />
			<IMPORT NAME="jdbc_jena" MODULE="virtjdbc3.jar" REQUIRED="true" />
		</IMPORTS>
	</CODEGENERATION>

	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
	</RETURNS>


</COMPONENT>
